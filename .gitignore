# macawi-ai/.gitignore

# Build artifacts
/target/
/build/
/dist/
*.o
*.so
*.dylib
*.exe

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS
.DS_Store
Thumbs.db

# Logs
*.log
logs/

# Environment
.env
.env.local
.env.*.local

# Dependencies
node_modules/
__pycache__/
*.pyc
.pytest_cache/

